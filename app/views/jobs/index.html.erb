All Jobs for <strong><%= @company.name %></strong>

<% @jobs.each do |job| %>
  <ul><%= job.title %> at <%= job.company.name %></ul>
  <ul><%= link_to "Profile", company_job_path(@company, job) %></ul>
<% end %>
<p><%= link_to "Add Job", new_company_job_path %></p>
<p><%= link_to "Back to All Companies", companies_path %></p>
